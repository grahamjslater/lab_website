@article{Law:2019aa,
	Annote = {doi: 10.1111/evo.13702},
	Author = {Law, Chris J. and \textbf{Slater, G. J.} and Mehta, Rita S.},
	Booktitle = {Evolution},
	Da = {2019/04/01},
	Date = {2019/04/01},
	Date-Added = {2019-04-22 11:12:28 -0500},
