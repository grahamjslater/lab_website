@article{Jaffe2011,
	Annote = {10.1098/rsbl.2010.1084},
	Author = {{Jaffe, A. L.*} and \textbf{Slater, G. J.} and Alfaro, Michael E.},
	Date = {2011/08/23},
	Date-Added = {2015-07-15 00:30:43 +0000},
